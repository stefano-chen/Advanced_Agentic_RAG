{
    "db_dir_path": "./store",
    "vector_db": "faiss",
    "check_input_validity": true,
    "simple_rag": false,
    "llm_provider": "google",
    "llm_model": "gemini-2.5-flash-lite",
    "llm_host": "",
    "embedding_provider": "huggingface",
    "embedding_model": "sentence-transformers/all-mpnet-base-v2",
    "embedding_host": "",
    "k": 7,
    "preprocessing_strategies": ["step-back", "hyde"],
    "preprocessing_strategies_options": {
        "step-back": {
            "max_tokens": 300
        },
        "hyde": {
            "max_tokens": 300
        }
    },
    "reranking_strategies": ["semantic"],
    "reranking_strategies_options": {
        "semantic": {
            "model_provider": "lm-studio",
            "model_name": "qwen/qwen3-4b",
            "model_host": "http://localhost:9999/v1"
        }
    },
    "selection_type": ["topk"],
    "selection_options": {
        "topk":{
            "k": 4
        }
    },
    "check_output_response_validity": true
}