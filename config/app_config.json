{
    "db_dir_path": "./store",
    "vector_db": "faiss",
    "check_input_validity": true,
    "simple_rag": false,
    "llm": {
        "llm_provider": "lm-studio",
        "llm_model": "google/gemma-2-9b",
        "llm_host": "http://localhost:9999/v1"
    },
    "embedding": {
        "embedding_provider": "huggingface",
        "embedding_model": "sentence-transformers/all-mpnet-base-v2",
        "embedding_host": ""
    },
    "k": 5,
    "query_transform": "step-back",
    "query_transform_params": {
        "max_char": 100
    },
    "reranking_strategies": ["semantic", "distance"],
    "reranking_weights": [0.7, 0.3],
    "reranking_strategies_options": {
        "semantic": {
            "llm_provider": "lm-studio",
            "llm_model": "google/gemma-2-9b",
            "llm_host": "http://localhost:9999/v1"
        },
        "distance": {
            "embedding_provider": "huggingface",
            "embedding_model": "sentence-transformers/all-mpnet-base-v2",
            "embedding_host": ""
        }
    },
    "selection_strategies": ["threshold", "topk"],
    "selection_options": {
        "threshold":{
            "min": 0.6
        },
        "topk":{
            "k": 4
        }
    },
    "check_output_response_validity": true
}